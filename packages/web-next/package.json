  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/*.(test|spec).(ts|tsx|js|jsx)"
    ],
    "collectCoverageFrom": [
      "app/**/*.(ts|tsx)",
      "components/**/*.(ts|tsx)",
      "lib/**/*.(ts|tsx)",
      "services/**/*.(ts|tsx)",
      "!**/*.d.ts",
      "!app/layout.tsx",
      "!app/globals.css"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura",
      "html"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
